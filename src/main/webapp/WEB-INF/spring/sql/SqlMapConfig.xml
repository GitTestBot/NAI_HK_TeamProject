<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
  <configuration>
  <!-- 	dto 객체의 변수설정 -->
  	<typeAliases>
  		<typeAlias type="com.hk.nai.dtos.AuthDto" alias="AuthDto"/>
  	  	<typeAlias type="com.hk.nai.dtos.MemberDto" alias="MemberDto"/>
  	  	<typeAlias type="com.hk.nai.dtos.commentDto" alias="CommentDto"/>
  		<typeAlias type="com.hk.nai.dtos.MessageDto" alias="MessageDto"/>
  		<typeAlias type="com.hk.nai.dtos.BoardDto" alias="BoardDto"/>
  		<typeAlias type="com.hk.nai.dtos.CommentDto1" alias="CommentDto1"/>
  	</typeAliases>
  	
  <!--dao 객체에서 쿼리 실행할 때 쿼리가 정의되어있는 xml 경로 설정  -->
  	<mappers>
  		<mapper resource="/sqls/mapper.xml"/>
  	</mappers>
  
  
  </configuration>